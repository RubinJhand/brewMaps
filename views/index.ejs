    <h1>My Map 1</h1>
    <div id="map"></div>
    <script>
         // Initialize and add the map
         function initMap() {
          // The location of Uluru
          var saskatoon = { lat: 52.1332, lng: -106.6700};
          // The map, centered at Uluru
          var map = new google.maps.Map(
            document.getElementById('map'), { zoom: 14, center: saskatoon });
          // The marker, positioned at Uluru
          var marker = new google.maps.Marker({ position: saskatoon, map: map });
        }
    </script>
    <script async defer
    src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBz-6_EJJAOq3Wayf8Q-hot5YYzz5T4o7M&callback=initMap">
